{
  "name": "functions",
  "scripts": {
    "lint": "./node_modules/.bin/tslint -p tslint.json",
    "build": "./node_modules/.bin/tsc",
    "serve": "npm run build && firebase serve --only functions",
    "shell": "npm run build && firebase experimental:functions:shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log",
    "compile": "tsc --declaration && node build/bootstrap.js && npm run build && firebase experimental:functions:shell",
    "dev": "nodemon -e ts  --exec \"npm run compile\"",
    "compiletest": "tsc --declaration && node build/test.js",
    "devtest": "nodemon -e ts  --exec \"npm run compiletest\"",
    "start": "nodemon -e ts  --exec \"npm run compile\"",
    "test": "tsc --declaration && node build/test/index.js",
    "mytest": "tsc --declaration && node build/test/test.js",
    "tsc": "tsc",
    "tsc:w": "tsc -w"
  },
  "main": "build/bootstrap.js",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/kaveh981/shareit.git"
  },
  "author": "Kaveh Fereidouni",
  "license": "ISC",
  "homepage": "https://bitbucket.org/kaveh981/shareit#readme",
  "dependencies": {
    "@types/body-parser": "^1.16.5",
    "@types/express": "^4.0.39",
    "@types/node": "^8.0.44",
    "bluebird": "^3.5.1",
    "body-parser": "^1.18.2",
    "commander": "^2.12.2",
    "deep-diff": "^0.3.8",
    "express": "^4.16.2",
    "express-jwt": "^5.3.0",
    "firebase": "^4.9.0",
    "firebase-admin": "~5.8.1",
    "firebase-functions": "^0.8.1",
    "fs-finder": "^1.8.1",
    "g": "^2.0.1",
    "inversify": "^4.5.2",
    "inversify-express-utils": "^4.0.1",
    "jsonwebtoken": "^8.1.1",
    "keypress": "^0.2.1",
    "knex": "^0.13.0",
    "mysql": "^2.15.0",
    "passport": "^0.4.0",
    "passport-jwt": "^3.0.1",
    "pg": "^7.3.0",
    "reflect-metadata": "^0.1.10",
    "request": "^2.83.0",
    "tape": "^4.8.0",
    "typeorm": "^0.1.11"
  },
  "devDependencies": {
