{
  "hosting": {
    "public": "public",
    "rewrites": [
      {
        "source": "/**",
        "function": "app"
      }
    ],
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ]
  },
  "functions": {
    "predeploy": [
      "npm --prefix $RESOURCE_DIR run build"
    ]
  }
}
// "npm --prefix $RESOURCE_DIR run lint",